{
    "cfconfigfile":"./.cfconfig.json",
    "app":{
        "cfengine":"${CFENGINE:lucee@5.3.4+80}"
    },
    "web":{
        "rewrites":{
            "enable":"true"
        }
    },
    "fusionreactor":{
        "enable":"${FR_ENABLE:false}",
        "port":"${FR_PORT:8088}",
        "licenseKey":"${FR_LICENSE_KEY}",
        "password":"${FR_PASSWORD}",
        "RESTRegisterHostname":"${FR_RESTREGISTERHOSTNAME:luceeTestApp}",
        "licenseDeactivateOnShutdown":"${FR_DEACTIVATEONSHUTDOWN:true}"
    }
}